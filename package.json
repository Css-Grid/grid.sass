{
    "name": "grid.sass",
    "version": "1.0.0",
    "description": "Easy to use grid system for all common browsers including IE 11.",
    "main": "sass/grid.sass",
    "repository": {
        "type": "git",
        "url": "https://github.com/danielneubert/grid.sass.git"
    },
    "bugs": {
        "url": "https://github.com/danielneubert/grid.sass/issues"
    },
    "license": "MIT",
    "author": "Daniel Neubert <git@danielneubert.com>",
    "homepage": "https://danielneubert.com/",
    "files": [
        "LICENSE.md",
        "grid.sass"
    ],
    "style": "css/grid.css",
    "sass": "sass/grid.sass",
    "keywords": [
        "grid",
        "css",
        "sass",
        "css-grid",
        "grid-sass",
        "display",
        "display-grid",
        "helper"
    ],
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "cross-env": "6.*",
        "laravel-mix": "5.*",
        "resolve-url-loader": "3*",
        "sass": "1.*",
        "sass-loader": "8.*"
    }
}
